    <!-- Joint position controllers (Gazebo Sim) -->
    <!-- J1 -->
    <plugin filename="libgz-sim-joint-position-controller-system.so"
            name="gz::sim::systems::JointPositionController">
      <joint_name>panda_joint1</joint_name>
      <p_gain>200</p_gain><d_gain>20</d_gain><i_gain>0</i_gain>
      <i_min>-1</i_min><i_max>1</i_max>
      <cmd_min>-100</cmd_min><cmd_max>100</cmd_max>
      <topic>/model/panda/joint/panda_joint1/cmd_pos</topic>
    </plugin>

