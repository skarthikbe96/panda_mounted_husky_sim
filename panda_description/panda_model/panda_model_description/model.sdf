    <!-- Fixed joint from link7 to the camera link, located at the gripper_mount frame -->
    <joint name="wrist_cam_mount" type="fixed">
      <parent>panda_link7</parent>        <!-- must be a LINK -->
      <child>wrist_cam_link</child>
      <pose relative_to="gripper_mount">0 0 0 0 0 0</pose>
    </joint>

    <link name="wrist_cam_link">
      <!-- push the camera forward 5 cm from the mount frame -->
      <pose relative_to="wrist_cam_mount">0 0 0.05 0 0 0</pose>

      <inertial>
        <mass>0.08</mass>
        <inertia>
          <ixx>1e-4</ixx><iyy>1e-4</iyy><izz>1e-4</izz>
          <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
        </inertia>
      </inertial>

      <visual name="wrist_cam_debug">
        <geometry><box><size>0.06 0.02 0.02</size></box></geometry>
        <material>
          <diffuse>0 1 0 1</diffuse>
          <ambient>0.3 0.3 0.3 1</ambient>
        </material>
      </visual>

      <!-- RGB-D sensor -->
      <sensor name="wrist_rgbd" type="rgbd_camera">
        <pose>0.015 0 0 0 -1.57079632679 0</pose>
        <update_rate>30</update_rate>
        <always_on>true</always_on>
        <visualize>true</visualize>
        <camera>
          <horizontal_fov>1.0471975512</horizontal_fov>
          <image><width>640</width><height>480</height><format>R8G8B8</format></image>
          <clip><near>0.10</near><far>5.0</far></clip>
        </camera>
        <!-- Gazebo base topic (GZ will append /image, /depth_image, /camera_info, /points) -->
        <topic>/panda/wrist/rgbd</topic>
      </sensor>
    </link>

